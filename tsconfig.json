{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "@assets/*": ["./src/assets/*"],
            "@constants/*": ["./src/constants/*"],
            "@typings/*": ["./src/typings/*"],
            "@screens/*": ["./src/screens/*"],
            "@routes/*": ["./src/routes/*"],
            "@components/*": ["./src/components/*"],
            "@libs/*": ["./src/libs/*"],
            "@services/*": ["./src/services/*"],
            "@contexts/*": ["./src/contexts/*"]
        }
    }
}
